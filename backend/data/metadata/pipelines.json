{
    "metasense": {
        "id": "metasense",
        "name": "MetaSense",
        "categories": [
            "rna-seq"
        ],
        "fileTypes": {
            "allowed": [
                ".fastq",
                ".fq",
                "application/gzip",
                "application/x-gzip"
            ],
            "match": [
                ".fastq.gz",
                ".fq.gz",
                ".fastq",
                ".fq"
            ]
        },
        "pluginType": "regular",
        "description": "Taxonomic profiling of mNGS datasets",
        "repository": "mshunjan/metasense",
        "user_inputs": {
            "input": {
                "command": "--input",
                "mandatory": true
            }
        },
        "predefined_inputs": [
            "--kraken_db ./indexes/kraken_db.tar.gz"
        ],
        "report": "multiqc/multiqc_report.html",
        "processes": {
            "SAMPLESHEET_GENERATE": "",
            "SAMPLESHEET_CHECK": "",
            "UNPACK_DATABASE": "",
            "KRAKEN2_KRAKEN2": "",
            "BRACKEN_BRACKEN": "",
            "BRACKEN_FILTER": "",
            "BRACKEN_COMBINEBRACKENOUTPUTS": "",
            "BRACKEN_PLOT": "",
            "CUSTOM_DUMPSOFTWAREVERSIONS": "",
            "MULTIQC": ""
        }
    }
}